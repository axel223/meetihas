{
  "name": "Meetihas",
  "description": "Record Google Meet Transcripts",
  "version": "0.1",
  "icons": {
    "128": "icons128.png",
    "64" :"icons64.png"
  },
  "content_scripts":[
    {
      "matches":["https://meet.google.com/*"],
      "js":["./jquery.min.js","inject.js"]
    }
  ],
    "permissions":[
      "tabs","storage","notifications"
  ],
  "host_permissions": [
    "https://meet.google.com/*"
  ],
  "manifest_version": 3
}
  