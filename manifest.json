{
    "name": "Meetihas",
    "description": "Record Google Meet Transcripts",
    "version": "0.1",
    "icons": {
      "128": "icons128.png",
      "64" :"icons64.png"
    },
    "content_scripts": [{
      "matches": ["https://meet.google.com/*"],
      "js": ["inject.js"],
      "run_at": "document_idle",
      "all_frames": false
    }],
    "manifest_version": 3
  }
  